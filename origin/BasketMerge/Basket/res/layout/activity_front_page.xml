<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".FrontPageActivity" >

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignRight="@+id/bEditCreditCards"
        android:layout_below="@+id/bEditCreditCards"
        android:layout_marginTop="22dp"
        android:text="basket list" />
    <Button
        android:id="@+id/bHomePage" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"       
        android:layout_marginTop="18dp"
        android:text="Home Page" />

	<Button
		android:id="@+id/button1"
		android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/AdminButton"
	    android:layout_marginLeft="25dp"
	    android:layout_marginTop="14dp"
	    android:layout_toRightOf="@+id/orderlistbutton"
	    android:text="My Picknick" />
	<Button
		android:id="@+id/bEditCreditCards"
		android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/AdminButton"
	    android:layout_marginLeft="25dp"
	    android:layout_marginTop="14dp"
	    android:layout_toRightOf="@+id/orderlistbutton"
	    android:text="My cc edit" />
				    



	<Button
	    android:id="@+id/AdminButton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBaseline="@+id/bHomePage"
	    android:layout_alignBottom="@+id/bHomePage"
	    android:layout_alignLeft="@+id/button3"
	    android:layout_marginLeft="14dp"
	    android:text="Admin" />

	<Button
	    android:id="@+id/productListButton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBaseline="@+id/button3"
	    android:layout_alignBottom="@+id/button3"
	    android:layout_alignLeft="@+id/orderlistbutton"
	    android:text="product list" />

	<Button
	    android:id="@+id/orderlistbutton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_above="@+id/button3"
	    android:layout_alignRight="@+id/bHomePage"
	    android:text="My Orders" />

	<Button
	    android:id="@+id/checkoutButton"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/productListButton"
	    android:layout_below="@+id/productListButton"
	    android:layout_marginLeft="14dp"
	    android:layout_marginTop="36dp"
	    android:text="Checkout" />
	<TextView
        android:id="@+id/tvCatTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="@string/hello_world" />
    <Button
        android:id="@+id/bCatePage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Category Page" />

    <Button
        android:id="@+id/bCateList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Category List" />

    <Button
        android:id="@+id/bHomePage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Home Page" />
</LinearLayout>
